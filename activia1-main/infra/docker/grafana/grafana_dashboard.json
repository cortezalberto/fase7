{
  "dashboard": {
    "id": null,
    "uid": "ai-native-mvp",
    "title": "AI-Native MVP - Observability Dashboard",
    "tags": ["ai-native", "education", "llm"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Total Interactions (Last Hour)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(ai_native_interactions_total)",
            "legendFormat": "Total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            },
            "unit": "none"
          }
        }
      },
      {
        "id": 2,
        "title": "Interaction Rate (requests/min)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "rate(ai_native_interactions_total[1m]) * 60",
            "legendFormat": "Rate",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "decimals": 2,
            "unit": "reqpm"
          }
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "ai_native_cache_hit_rate_percent{cache_type=\"llm\"}",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Database Pool Usage",
        "type": "gauge",
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "(ai_native_database_pool_checked_out / ai_native_database_pool_size) * 100",
            "legendFormat": "Usage %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "LLM Call Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(ai_native_llm_call_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ai_native_llm_call_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(ai_native_llm_call_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "logBase": 1,
            "show": true
          }
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true
        }
      },
      {
        "id": 6,
        "title": "Database Query Duration (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ai_native_database_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{operation}} on {{table}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "show": true
          }
        ]
      },
      {
        "id": 7,
        "title": "Interactions by Agent",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "sum by (agent_used) (ai_native_interactions_total)",
            "legendFormat": "{{agent_used}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Cognitive States Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 14},
        "targets": [
          {
            "expr": "sum by (cognitive_state) (ai_native_cognitive_states_total)",
            "legendFormat": "{{cognitive_state}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Risks Detected by Level",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 14},
        "targets": [
          {
            "expr": "sum by (risk_level) (ai_native_risks_detected_total)",
            "legendFormat": "{{risk_level}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "orange"},
                {"value": 20, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Governance Blocks Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "rate(ai_native_governance_blocks_total[5m]) * 300",
            "legendFormat": "{{reason}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Blocks/5min",
            "show": true
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [10],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "name": "High Governance Block Rate",
          "message": "Governance is blocking more than 10 requests per 5 minutes"
        }
      },
      {
        "id": 11,
        "title": "N4 Traces Created",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "rate(ai_native_traces_created_total[5m]) * 300",
            "legendFormat": "{{trace_level}} - {{interaction_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Traces/5min",
            "show": true
          }
        ]
      }
    ]
  },
  "overwrite": false
}